{
    "name": "drewlabs/php-value",
    "description": "PHP object builder and serialization library",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Drewlabs\\PHPValue\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\PHPValue\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2|^7.3|^7.4|^8.0|^8.1",
        "drewlabs/core-helpers": "^0.2",
        "symfony/polyfill-php81": "^1.24"
    },
    "keywords": [
        "built type",
        "cast",
        "serialization"
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
