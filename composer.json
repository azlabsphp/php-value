{
    "name": "drewlabs/immutable",
    "description": "PHP object immutability implementation",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [{
        "name": "azandrew-sidoine",
        "email": "asmyns.platonnas29@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Drewlabs\\PhpValue\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\PhpValue\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^7.4|^8.0|^8.1",
        "drewlabs/core-helpers": "^2.0",
        "drewlabs/contracts": "^2.0",
        "symfony/polyfill-php81": "^1.24"
    },
    "repositories": [{
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-contracts.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core-helpers.git"
        }
    ],
    "keywords": [
        "immutable"
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}